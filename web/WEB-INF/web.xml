<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <servlet>
        <servlet-name>RemoveFromCartServlet</servlet-name>
        <servlet-class>foo.servlet.RemoveFromCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownZipServlet</servlet-name>
        <servlet-class>foo.servlet.DownZipServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuccessServlet</servlet-name>
        <servlet-class>foo.servlet.SuccessServlet</servlet-class>
    </servlet>
    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>
</web-app>
